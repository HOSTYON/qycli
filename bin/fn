#!/bin/bash

# function library for qyc

wp() {
    while [[ "$#" -gt 0 ]]
    do
        case "$1" in
            -o|option)
                option="$2"
                shift
                ;;
            length)
                length="$2"
                shift
                ;;
            *)
                echo "Invalid argument: $1"
                print_help_wp
                ;;
        esac
        shift
    done
    echo "option is $option"
    echo "length is $length"
}

mk() {
    if [[ "$1" == "wp" ]]; then
        shift
        wp "$@"
        else
        print_help_mk
    fi
}

